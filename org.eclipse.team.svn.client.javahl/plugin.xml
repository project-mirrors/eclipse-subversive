<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.team.svn.client.javahl"
   name="%pluginName"
   version="1.1.2"
   provider-name="%providerName"
   class="org.eclipse.team.svn.client.javahl.JavaHLPlugin">

	<runtime>
		<library name="javahl_client.jar">
			<export name="*"/>
		</library>
		<library name="lib/svnjavahl.jar">
			<export name="*"/>
		</library>
	</runtime>
   
	<requires>
		<import plugin="org.eclipse.team.svn.core"/>
		<import plugin="org.eclipse.core.runtime"/>
	</requires>
   
	<extension point="org.eclipse.team.svn.core.svnclient">
		<svnclient class="org.eclipse.team.svn.client.javahl.JavaHLClientWrapperFactory"/>
	</extension>
	
</plugin>
